<% if messageboard %>
  <% cache(["currently_online", messageboard.id], expires_in: 1.minute) do %>
    <div class="online">
      <h3>Currently Online</h3>
      <ul>
        <% messageboard.active_users.each do |u| %>
         <li><%= u.name %></li>
        <% end %>
      </ul>
    </div>
  <% end -%>
<% end %>
